server {
	
	listen 80;
	server_name prospero.jonmorehouse.me;

	location /resources/ {

		root /home/ubuntu/prospero/;
	
	}

	location /property_images/ {

                root /home/ubuntu/prospero/;
	

	}
	
	location /property_pdfs/ {
	
                root /home/ubuntu/prospero/;

	}

	location / {

        proxy_pass http://localhost:8888;
        include /etc/nginx/conf/proxy.conf;

	}
}
