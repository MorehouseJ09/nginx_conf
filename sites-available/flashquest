server {
		
	listen 8080;
	
	server_name flashquest;

	location / {

		proxy_pass http://127.0.0.1:3000;
        include /usr/local/nginx/conf/proxy.conf;
	}
}

server {
	
	listen 8080;

	server_name temp;

	location / {

		proxy_pass http://0.0.0:4567;
		include /usr/local/nginx/conf/proxy.conf;
	}

}
